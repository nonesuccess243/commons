<client name="tjjsw" default-encoding="GBK">

	<request name="get_register_populationInfo" url="hjrk.do" endcoding="GBK">
		<default-param name="method" value="list" />
		<default-param name="par" value="del" />
		<pattern>	
        <![CDATA[
		户别
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		#{{td}}[\s\n]${censusRegisterCategory}[\s\n]@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		户口性质
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		#{{td}}[\s\n]{0,10000}?${censusRegisterNature}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		公安户口簿号
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		#{{td}}[\s\n]{0,10000}?${censusRegisterNumber}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		户籍地代码
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		#{{td}}[\s\n]{0,10000}?${censusRegisterCode}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		户籍地地址
		]]>
		</pattern>
		<pattern>
		 <![CDATA[
		#{{td}}[\s\n]{0,10000}?${censusRegisterAddress}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		现居地代码
		]]>
		</pattern>
		<pattern>
        <![CDATA[
       	#{{td}}[\s\n]{0,10000}?${censusLivingNowCode}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		现居地地址
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
        #{{td}}[\s\n]{0,10000}?${censusLivingNowAddress}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		门牌号
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		 #{{td}}[\s\n]{0,10000}?${censusTelephone}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		联系电话
		]]>
		</pattern>
		<pattern>	
        <![CDATA[
		 #{{td}}[\s\n]{0,10000}?${censusPhone}[\s\n]{0,10000}?@{{td}}
		]]>
		</pattern>
	</request>
	
</client>
